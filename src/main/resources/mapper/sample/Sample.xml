<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
   
<mapper namespace="mil.gdl.nis.sample.mapper.SampleMapper">
  <select id="selectSampleList" resultType="hashmap">
     <![CDATA[
     	SELECT
     		  PRODUCT_ID
     		, PRODUCT_NAME
